{
    "name": "LeveeMonitorP1",
    "functions": [ {
        "name": "getLeveeState",
        "module": "functions/ismop/LMonFunctions.js"
    }, {
        "name": "computeThreatLevel",
        "module": "functions/ismop/LMonFunctions.js"
    }, {
        "name": "severeEmergencyActions",
        "module": "functions/ismop/LMonFunctions.js"
    } ],
    "processes": [ {
        "name": "LeveeStateChecker",
	"type": "choice",
        "function": "getLeveeState",
        "config": { "levee_id": 1},
	"firingInterval": 5000,
        "ins": [ ],
        "outs": [ "ELHightened", "ELSevere" ]
    }, {
        "name": "ThreatLevelComputation",
	"type": "dataflow",
        "function": "computeThreatLevel",
        "config": { "levee_id": 1},
        "ins": [ "ELHightened" ],
        "outs": [ ]
    }, {
        "name": "SevereEmergency",
	"type": "dataflow",
        "function": "severeEmergencyActions",
        "ins": [ "ELSevere" ],
        "outs": [ ]
    } ],
    "signals": [ {
        "name": "ELHightened"
    }, {
        "name": "ELSevere"
    } ],
    "ins": [ ],
    "outs": [ ]
}
